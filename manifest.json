{
  "name": "Github page switcher",
  "version": "0.3.0",
  "manifest_version": 3,
  "description": "show one-touch button between github repository and github page",
  "icons": {
    "16": "icon_016.png",
    "128": "icon_128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*",
        "*://*.github.io/*"
      ],
      "js": [
        "page-switcher.js"
      ],
      "css": []
    }
  ],
  "permissions": [],
  "host_permissions": [
    "*://github.com/*",
    "*://*.github.io/*"
  ]
}
