{
  "name": "Github page switcher",
  "version": "0.2.0",
  "manifest_version": 2,
  "description": "show one-touch button between github repository and github page",
  "icons": {
    "16": "icon_016.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*",
        "https://*.github.io/*",
        "http://*.github.io/*"
      ],
      "js": [
        "page-switcher.js"
      ],
      "css": []
    }
  ],
  "permissions": []
}
